    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^4.6.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.1",
    "routing-controllers-openapi": "^4.0.0",
    "sequelize": "^6.26.0",
    "sequelize-typescript": "^2.1.5",
    "sqlite3": "^5.1.2",
